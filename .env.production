# 生产环境配置
NODE_ENV=production
REACT_APP_DFX_NETWORK=ic
REACT_APP_IC_HOST=https://ic0.app

# 功能开关
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_ENABLE_REAL_TRADING=true

# 性能配置
REACT_APP_PRICE_UPDATE_INTERVAL=10000
REACT_APP_MAX_LEVERAGE=10

# 安全配置
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_MAX_RETRY_ATTEMPTS=3

# UI配置
REACT_APP_THEME_MODE=auto
REACT_APP_DEFAULT_LANGUAGE=EN

# 监控配置
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
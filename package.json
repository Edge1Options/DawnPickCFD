{
  "name": "dawnpick-cfd",
  "version": "1.0.0",
  "description": "DawnPick CFD Trading Platform",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:production": "NODE_ENV=production REACT_APP_DFX_NETWORK=ic npm run build",
    "deploy:local": "dfx deploy --network local",
    "deploy:ic": "dfx deploy --network ic",
    "deploy:frontend:local": "npm run build && dfx deploy dawnpick_frontend --network local",
    "deploy:frontend:ic": "npm run build:production && dfx deploy dawnpick_frontend --network ic --with-cycles 2000000000000",
    "deploy:quick": "./quick-deploy.sh",
    "deploy:update": "./update-deployment.sh",
    "deploy:existing-user": "./deploy-frontend-ic-existing-user.sh",
    "deploy:full": "npm run build && dfx deploy --network local",
    "deploy:production": "npm run build:production && dfx deploy --network ic --with-cycles 2000000000000",
    "predeploy": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist .dfx",
    "clean:build": "rm -rf dist",
    "clean:dfx": "rm -rf .dfx",
    "setup": "npm install && dfx start --background --clean",
    "setup:ic": "dfx identity deploy-wallet --network ic",
    "stop": "dfx stop",
    "status": "dfx canister status --all --network local",
    "status:ic": "dfx canister status --all --network ic",
    "logs": "dfx canister logs dawnpick_frontend --network local",
    "logs:ic": "dfx canister logs dawnpick_frontend --network ic",
    "canister:info": "dfx canister id --all --network local",
    "canister:info:ic": "dfx canister id --all --network ic",
    "wallet:balance": "dfx wallet balance --network ic",
    "identity:whoami": "dfx identity whoami",
    "identity:list": "dfx identity list",
    "check:deployment": "./check-deployment.sh"
  },
  "dependencies": {
    "@dfinity/agent": "^0.19.3",
    "@dfinity/auth-client": "^0.19.3",
    "@dfinity/candid": "^0.19.3",
    "@dfinity/principal": "^0.19.3",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "js-sha256": "^0.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.6.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  }
}
